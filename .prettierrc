{
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "plugins": [
        "prettier-plugin-go-template"
    ],
    "overrides": [
        {
            "files": [
                "*.html"
            ],
            "options": {
                "parser": "go-template"
            }
        }
    ]
}